{
  "name": "zonia-warehouse",
  "version": "0.0.1",
  "author": "Maciej Zonski <me@zonni.pl>",
  "description": "Warehouse management tool",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/mzonski/zonia-warehouse",
    "type": "git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-clean": "yarn start --reset-cache",
    "clean": "watchman watch-del-all && rd /s /q \"node_modules/\" && yarn cache clean",
    "clean-run": "watchman watch-del-all && rd /s /q \"node_modules/\" && yarn cache clean && yarn install && yarn start -- --reset-cache",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx}",
    "lint": "tsc --noEmit && eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "build-qa:android": "cd android && ENVFILE=.env ./gradlew assembleQaRelease",
    "run-qa:android": "ENVFILE=.env react-native run-android --variant=qa",
    "build-production:android": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "run-production:android": "ENVFILE=.env.production react-native run-android --variant=release",
    "prepare": "husky install",
    "android-adb:forward-ports": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.3.1",
    "@react-native-async-storage/async-storage": "^1.17.5",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@rneui/base": "^4.0.0-rc.4",
    "@rneui/themed": "^4.0.0-rc.4",
    "axios": "^0.27.2",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "color": "^4.2.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-native": "0.67.4",
    "react-native-col": "^1.1.5",
    "react-native-config": "^1.4.5",
    "react-native-flipper": "0.143.0",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-get-random-values": "^1.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-paper": "^4.12.0",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "3.4.1",
    "react-native-screens": "^3.13.1",
    "react-native-snackbar": "^2.4.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-vision-camera": "^2.13.2",
    "react-redux": "^7.2.8",
    "redux": "^4.2.0",
    "redux-flipper": "^2.0.1",
    "use-async-effect": "^2.2.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@types/lodash": "^4.14.182",
    "@types/react-native": "0.67.5",
    "@types/react-redux": "^7.1.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "metro-react-native-babel-preset": "0.67.0",
    "prettier": "^2.6.2",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  }
}
